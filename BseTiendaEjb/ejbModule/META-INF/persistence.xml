<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="file:///persistence_2_0.xsd http://java.sun.com/xml/ns/persistence">

   	<persistence-unit name="bselatiendaws-pu">

		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
 	    <jta-data-source>java:/BSEWSRECTOR</jta-data-source> 
	    
 	    <properties>
			<property name="jdbc.use_getGeneratedKeys" value="true" />
		   	<property name="eclipselink.target-server" value="JBoss"/>
			<property name="eclipselink.weaving" value="static" />
			<property name="eclipselink.allow-zero-id" value="true" />
		</properties>
	</persistence-unit>
</persistence>
